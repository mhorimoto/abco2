// Ethernet initialize
//  MAC Address store in EEPROM 4090-4095

#include <EEPROM.h>
#include <SPI.h>
#include <Ehternet2.h>
#define  MAC_ADDR_PTR   0xffa   // DEC=4090

byte mac[6];
byte ipa[4];

void setup() {
  int i;
  pinMode(13,OUTPUT);
  digitalWrite(13,LOW);
  for(i=0;i<6;i++) {
    mac[i] = EEPROM.read(MAC_ADDR_PTR+i);
  }
  Ethernet.begin(mac);
  
